- content_for :board do
  = javascript_include_tag 'application', 'data-turbolinks-track' => true
  #pins.transitions-enabled
    - @pins.each do |pin|
      .box.panel.panel-default
        = link_to (image_tag pin.image.url), pin
        %h2
          = link_to pin.title, pin
        %p.user
          Ofertado por
          = pin.user.name
        %p.price
          $
          = pin.price